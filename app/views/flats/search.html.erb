<div class="container">
  <div class="row">
    <div class="col-xs-6">
    <% @flats.each do |flat| %>
      <%= link_to flat.presentation, user_flat_path(flat.user_id,flat) %>
      <ul class="list-inline">
      <li><%= flat.property_type %></li>
      <li><%= flat.city %></li>
      </ul>
    <% end %>
    </div>
  </div>
</div>



